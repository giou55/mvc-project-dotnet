@model IQueryable<Book>

<div class="text-center">
    <h1 class="display-4">Books</h1>
</div>

@foreach (var b in Model)
{
    <h3>@b.Title</h3>
    <p>@b.Author</p>
    <a href="~/Book/Details/@b.BookID">Details</a>
    <a href="~/Book/Edit/@b.BookID">Edit</a>
    <a href="~/Book/Delete/@b.BookID">Delete</a>
    <hr />
}
